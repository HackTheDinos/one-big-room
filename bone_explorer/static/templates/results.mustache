{{#results_list}}
<div class="media">
  <div class="media-left">
    <div class="media-object">
      <img src="{{imageUrl}}" alt="" />
      {{>slice_preview}}      
    </div>
  </div>
  <div class="media-body">
    <h3 class="media-heading">
      <a href="{{url}}">{{scientific_name}}</a>
      <small>{{commonName}}</small>
    </h3>
    {{>classification}}
    <a href="{{digimorph_url}}">Digimorph</a>
    {{wikipedia}}
  </div>
</div>
{{/results_list}}